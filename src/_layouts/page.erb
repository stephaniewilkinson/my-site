---
layout: default
---
<div class="dark:bg-transparent md:mt-16 mt-4 px-8 grid grid-cols-1 gap-y-16 lg:grid-cols-2 lg:gap-y-12 mx-auto max-w-2xl lg:max-w-7xl">
  <div class="">
    <h1 class="text-6xl font-bold tracking-tight text-zinc-800 dark:text-zinc-100 sm:text-6xl">
      <%= resource.data.title %>
    </h1>
    <p class="text-zinc-800 tracking-tight dark:text-zinc-100 text-3xl mb-8">
      <%= resource.data.subtitle %>
    </p>
    <span class="dark:text-zinc-400 mt-6 text-base text-zinc-600">
      <%= yield %>
    </span>
  </div>
  <div class="lg:pl-20">
    <div class="mb-12 max-w-2xl px-2.5 lg:max-w-none">
      <img alt="" sizes="(min-width: 1024px) 32rem, 20rem" src="<%= relative_url '/images/headshot.jpg' %>" decoding="async" data-nimg="1" class="aspect-square rotate-3 rounded-3xl object-cover" loading="lazy" style="color: transparent;">
    </div>
    <%= render "social_media" %>
  </div>
</div>
